<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LoginMenuController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <TextField fx:id="textuser" layoutX="246.0" layoutY="102.0" promptText="username" />
            <Button fx:id="buttonlogin" layoutX="294.0" layoutY="237.0" mnemonicParsing="false" onAction="#buttonloginclick" text="Log in" />
            <Text fx:id="texttitle" layoutX="293.0" layoutY="61.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome!" />
            <PasswordField fx:id="textpass" layoutX="244.0" layoutY="142.0" promptText="password" />
            <Text layoutX="180.0" layoutY="119.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username:" />
            <Text layoutX="181.0" layoutY="159.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password:" />
      </children>
    </AnchorPane>
  </children>
</VBox>
