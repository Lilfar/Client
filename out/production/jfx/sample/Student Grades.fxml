<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="484.0" prefWidth="379.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.StudentGradesController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="484.0" prefWidth="383.0" style="-fx-background-color: lavender;" VBox.vgrow="ALWAYS">
         <children>
             <ImageView fitHeight="490.0" fitWidth="391.0" layoutX="-1.0" layoutY="-7.0" pickOnBounds="true">
                 <image>
                     <Image url="https://image.freepik.com/free-photo/school-supplies-stationery-accessories-wood-background_7192-926.jpg" />
                 </image>
             </ImageView>
            <TableView fx:id="studentgrades" layoutX="80.0" layoutY="64.0" prefHeight="334.0" prefWidth="234.0">
              <columns>
                <TableColumn fx:id="examname" prefWidth="154.39997100830078" text="Exam Name" />
                <TableColumn fx:id="examgrade" prefWidth="71.20001220703125" text="Grade" />
              </columns>
            </TableView>
            <Button fx:id="buttonback" layoutX="26.0" layoutY="416.0" mnemonicParsing="false" onAction="#buttonbackclick" text="Back">
               <font>
                  <Font name="Centaur" size="13.0" />
               </font>
            </Button>
            <Button fx:id="buttondownload" layoutX="264.0" layoutY="416.0" mnemonicParsing="false" onAction="#buttondownloadclick" text="Download exam">
               <font>
                  <Font name="Centaur" size="13.0" />
               </font>
            </Button>
         </children>
    </AnchorPane>
  </children>
</VBox>
