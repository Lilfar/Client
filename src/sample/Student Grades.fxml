<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.StudentGradesController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <TableView fx:id="studentgrades" layoutX="207.0" layoutY="100.0" prefHeight="200.0" prefWidth="225.0">
              <columns>
                <TableColumn fx:id="examname" prefWidth="108.800048828125" text="Exam Name" />
                <TableColumn fx:id="examgrade" prefWidth="115.99993896484375" text="Grade" />
              </columns>
            </TableView>
            <Button fx:id="buttonback" layoutX="207.0" layoutY="333.0" mnemonicParsing="false" onAction="#buttonbackclick" text="Back" />
            <Button fx:id="buttondownload" layoutX="331.0" layoutY="333.0" mnemonicParsing="false" onAction="#buttondownloadclick" text="Download exam" />
         </children>
    </AnchorPane>
  </children>
</VBox>
