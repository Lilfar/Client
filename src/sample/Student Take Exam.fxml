<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.StudentTakeExamController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <TextField fx:id="textcode" layoutX="246.0" layoutY="99.0" promptText="Enter exam code" />
            <Button fx:id="buttongo" layoutX="295.0" layoutY="175.0" mnemonicParsing="false" onAction="#buttongoclick" text="Go" />
            <Button fx:id="buttonback" layoutX="291.0" layoutY="280.0" mnemonicParsing="false" onAction="#buttonbackclick" text="Back" />
            <Text layoutX="179.0" layoutY="116.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Exam code:" />
            <Text layoutX="281.0" layoutY="69.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Good Luck!" />
         </children>
    </AnchorPane>
  </children>
</VBox>
