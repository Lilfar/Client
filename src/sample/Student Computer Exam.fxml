<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.StudentComputerExamController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Text layoutX="273.0" layoutY="58.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Exam in progress!" />
            <Button fx:id="buttonnext" layoutX="411.0" layoutY="326.0" mnemonicParsing="false" onAction="#buttonnextclick" text="Next" />
            <RadioButton fx:id="radioa4" layoutX="202.0" layoutY="271.0" mnemonicParsing="false" onAction="#radioa4click" prefHeight="0.0" prefWidth="34.0" text="a4" />
            <RadioButton fx:id="radioa1" layoutX="202.0" layoutY="200.0" mnemonicParsing="false" onAction="#radioa1click" text="a1" />
            <RadioButton fx:id="radioa3" layoutX="202.0" layoutY="247.0" mnemonicParsing="false" onAction="#radioa3click" text="a3" />
            <RadioButton fx:id="radioa2" layoutX="202.0" layoutY="222.0" mnemonicParsing="false" onAction="#radioa2click" text="a2" />
            <Text fx:id="textquestion" layoutX="194.0" layoutY="179.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Question:" />
            <Button fx:id="buttonprev" layoutX="168.0" layoutY="326.0" mnemonicParsing="false" onAction="#buttonprevclick" text="Prev" />
            <Text fx:id="texttimeremaining" layoutX="273.0" layoutY="96.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Time remaining: 90:00" />
         </children>
    </AnchorPane>
  </children>
</VBox>
