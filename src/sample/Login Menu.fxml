<?import javafx.scene.text.Text?>

<VBox prefHeight="450.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LoginMenuController">
  <children>
    <AnchorPane maxHeight="0.0" maxWidth="0.0" prefHeight="0.0" prefWidth="0.0" VBox.vgrow="ALWAYS">
      <children>
            <ImageView fx:id="Background" fitHeight="450.0" fitWidth="760.0" layoutX="-5.0" layoutY="2.0" preserveRatio="true">
               <image>
                  <Image url="https://image.freepik.com/free-vector/welcome-back-school-background_3589-817.jpg" />
               </image>
            </ImageView>
            <TextField fx:id="textuser" layoutX="320.0" layoutY="152.0" promptText="username" />
            <Button fx:id="buttonlogin" layoutX="320.0" layoutY="260.0" mnemonicParsing="false" onAction="#buttonloginclick" text="Log in" />
            <Text fx:id="texttitle" layoutX="296.0" layoutY="96.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome!" wrappingWidth="166.58636474609375">
               <font>
                  <Font name="David Bold" size="37.0" />
               </font></Text>
            <Text layoutX="236.0" layoutY="169.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username:" />
            <Text layoutX="238.0" layoutY="210.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password:" />
            <PasswordField fx:id="textpass" layoutX="320.0" layoutY="193.0" promptText="password" />
      </children>
    </AnchorPane>
  </children>
</VBox>
